{
    "name": "skeeks/yii2-cms",
    "description": "Yii2 cms extension",
    "keywords": ["framework", "cms", "skeeks", "sx", "app", "yii2", "extension"],
    "homepage": "http://www.skeeks.com/",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "http://www.skeeks.com/",
        "wiki": "http://cms.skeeks.com/wiki/",
        "source": "http://git.skeeks.com/skeeks/yii2-cms"
    },
    "authors": [
        {
            "name": "Semenov Alexander",
            "email": "semenov@skeeks.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "skeeks/yii2-sx": "*",
        "skeeks/yii2-widget-simpleajaxuploader": "*",
        "skeeks/yii2-widget-ckeditor": "*",
        "skeeks/yii2-widget-chosen": "*",
        "miloschuman/yii2-highcharts-widget": "dev-master"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {
            "skeeks\\cms\\": ""
        }
    }
}